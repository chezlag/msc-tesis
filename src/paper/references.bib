@article{achenbachChildBehaviorProfile1978,
  title = {The Child Behavior Profile: {{I}}. {{Boys}} Aged 6{\textendash}11.},
  author = {Achenbach, Thomas M},
  year = {1978},
  journal = {Journal of Consulting and Clinical psychology},
  volume = {46},
  number = {3},
  pages = {478},
  publisher = {{American Psychological Association}},
  abstract = {Attempted to develop a descriptive classification system that could be used to group children for research and clinical purposes, to reflect adaptive competencies as well as behavior problems, and to facilitate quantitative assessment of behavioral change. The system is embodied in a series of Child Behavior Profiles that are standardized separately for children of each sex at ages 4{\textendash}5, 6{\textendash}21, and 12{\textendash}26 yrs. The profiles are scored from the Child Behavior Checklist (CBCL), which was designed to obtain parents' reports of their children's competencies and problems in standardized format. The present article reports standardization of the profile for boys aged 6{\textendash}11 yrs. Factor analysis of the CBCLs of 450 disturbed boys yielded 9 behavior problem scales labeled Schizoid, Depressed, Uncommunicative, Obsessive{\textendash}Compulsive, Somatic Complaints, Social Withdrawal, Hyperactive, Aggressive, and Delinquent. The 1st 5 problem scales loaded on a 2nd-order factor labeled Internalizing, and the last 3 loaded on a factor labeled Externalizing. Three social competence scales entitled Activities, Social, and School were also constructed from the CBCL. Norms are based on a normal sample of 300. Comparison of disturbed and normal boys showed significant differences on all behavior problem and social competence scores. Eight-day test{\textendash}retest correlations averaged .89, whereas interparent correlations averaged .74. Computerized and hand-scored versions of the profile can be used to display item and scale scores for individual boys.}
}

