@article{luoAreSelfperceptionsAging2020,
  title = {Are {{Self-perceptions}} of {{Aging Associated With Health Trajectories Among Middle-Aged}} and {{Older Adults}}?},
  author = {Luo, Meng Sha and Li, Lydia W},
